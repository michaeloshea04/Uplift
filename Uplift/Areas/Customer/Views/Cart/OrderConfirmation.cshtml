@model int
@{
    ViewData["Title"] = "OrderConfirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-row">
    <div class="col-12 text-center">
        <h1 class="text-primary text-center">Order Request Submitted</h1><br/>
        <img src="/images/services/lawn_leaf.jpg" width="50%" />
    </div>
    <div class="col-12 text-center" style="color:maroon">
        <br />
        Thank you for submitting your request online! <br/>
        We have received your request and our team will be calling you shortly!
        <br/>
        Your request number is:  @Model

    </div>
</div>
